{
  "name": "http-status-typed",
  "version": "1.0.1",
  "description": "HTTP status codes, but in a typescript enum so you have a parent type",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "compress": "terser dist/index.js --compress --output dist/index.min.js",
    "publish": "npm run build && npm run compress && cp dist/index.min.js ./index.js && cp dist/index.d.ts ."
  },
  "keywords": [
    "node",
    "http",
    "apache",
    "HttpStatus",
    "httpclient",
    "status",
    "codes",
    "enum",
    "typescript",
    "react",
    "next"
  ],
  "author": "Shahaed Hasan",
  "license": "MIT",
  "devDependencies": {
    "terser": "^5.8.0",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shahaed/http-status-typed.git"
  }
}
